modules = ["nodejs-20", "web"]
[agent]
expertMode = true
integrations = ["javascript_openai:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["lsof", "psmisc"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Development Server"

[[workflows.workflow]]
name = "Development Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 3001
externalPort = 4200

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 33953
externalPort = 3001

[[ports]]
localPort = 38053
externalPort = 3003

[[ports]]
localPort = 39225
externalPort = 3002

[[ports]]
localPort = 40125
externalPort = 3000

[[ports]]
localPort = 41415
externalPort = 5173

[[ports]]
localPort = 41647
externalPort = 5000

[deployment]
deploymentTarget = "autoscale"
run = ["npm", "start"]
build = ["npm", "run", "build"]
